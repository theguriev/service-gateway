{
    "/test": {
        "methods": ["GET", "OPTIONS"],
        "proxy": {
            "to": "http://localhost:3000"
        }
    },
    "/test-authorization": {
        "methods": ["GET"],
        "authorizationNeeded": true,
        "proxy": {
            "to": "http://localhost:3000"
        }
    },
    "/test/:wallId/:id": {
        "methods": ["GET"],
        "proxy": {
            "to": "http://localhost:3000/:wallId/:id"
        }
    }
}
